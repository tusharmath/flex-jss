{
  "name": "flex-jss",
  "description": "jss utils for flexbox",
  "author": {
    "name": "Tushar Mathur &lt;tusharmath@gmail.com&gt;",
    "url": "http://tusharm.com"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "babel src --out-dir .dist --source-maps true",
    "lint": "eslint .",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "doc": "jsdoc2md ./src/** --global-index-format grouped --template README.template.md > README.md"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tusharmath/flex-jss.git"
  },
  "engines": {
    "node": ">= 6.0.0"
  },
  "main": ".dist/flex-jss.js",
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ]
    }
  },
  "keywords": [
    "jss",
    "flex",
    "css",
    "style",
    "grid",
    "box"
  ],
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.11.5",
    "cz-conventional-changelog": "^1.2.0",
    "eslint": "^3.3.1",
    "eslint-config-standard": "^6.0.0",
    "ghooks": "^1.3.2",
    "jsdoc-to-markdown": "^1.3.7",
    "semantic-release": "^4.3.5",
    "standard": "^8.0.0",
    "validate-commit-message": "^3.0.1"
  },
  "dependencies": {
    "jss": "^5.4.0",
    "jss-preset-default": "^1.2.0"
  }
}
